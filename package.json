{
  "name": "the-android-apps-project",
  "description": "The Android Apps Project Teaser Page",
  "version": "1.0.0",
  "author": "David A. Lindahl",
  "dependencies": {
    "gatsby": "^1.9.232",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-react-helmet": "^2.0.7",
    "gatsby-plugin-sass": "^1.0.21",
    "gatsby-plugin-typography": "^1.7.18",
    "postcss-cli": "^5.0.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography-theme-kirkham": "^0.16.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.js'"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1",
    "tailwindcss": "^0.5.1"
  }
}
